{
  "private": true,
  "dependencies": {
    "next": "latest",
    "react": "^15.4.2",
    "react-dom": "^15.4.2"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.22.0",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^6.10.3",
    "eslint-plugin-standard": "^3.0.1",
    "jest": "^19.0.2",
    "react-test-renderer": "^15.4.2"
  },
  "scripts": {
    "pretest": "eslint .",
    "test": "jest",
    "dev": "next",
    "postinstall": "next build",
    "start": "next start"
  },
  "engines": {
    "node": ">=5"
  },
  "babel": {
    "env": {
      "development": {
        "presets": [
          "next/babel"
        ]
      },
      "production": {
        "presets": [
          "next/babel"
        ]
      },
      "test": {
        "presets": [
          "es2015",
          "next/babel"
        ]
      }
    }
  },
  "eslintConfig": {
    "plugins": [
      "react"
    ],
    "extends": [
      "standard",
      "plugin:react/recommended"
    ],
    "rules": {
      "react/react-in-jsx-scope": 0
    }
  }
}
